{
  "domains": [
    "www.webbuzz.tech",
    "webbuzz.tech"
  ],
  "rewrites": [
    {
      "source": "/((?!.*\\.html$|.*\\.[a-zA-Z0-9]+$).*)",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "webbuzz-tech.vercel.app"
        }
      ],
      "destination": "https://www.webbuzz.tech/$1",
      "permanent": true
    },
    {
      "source": "/webbuzz.html",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/webbuzz",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/about-webbuzz",
      "destination": "/about-webbuzz.html",
      "permanent": true
    },
    { 
      "source": "/index.html", 
      "destination": "/", 
      "permanent": true 
    },
    {
      "source": "/:path+/",
      "destination": "/:path+",
      "permanent": true
    },
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "webbuzz.tech"
        }
      ],
      "destination": "https://www.webbuzz.tech/$1",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy", 
          "value": "strict-origin-when-cross-origin"
        }
      ]
    },
    {
      "source": "/",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    },
    {
      "source": "/(.*)\\.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "buildCommand": "vite build",
  "outputDirectory": "dist",
  "framework": "vite",
  "github": {
    "silent": true
  }
}